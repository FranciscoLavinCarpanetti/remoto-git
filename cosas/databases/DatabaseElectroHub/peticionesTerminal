mysql> SELECT cliente.Nombre, cliente.Apellidos, COUNT(pedidos.IdPedidos) AS TotalPedidos, SUM(pedidos.Total) AS TotalGastado
    -> FROM cliente
    -> JOIN pedidos ON cliente.IdDNI = pedidos.IdDNI
    -> GROUP BY cliente.IdDNI;
