mysql> SELECT cliente.Nombre, cliente.Apellidos, pedidos.Estado
    -> FROM cliente
    -> JOIN pedidos ON cliente.IdDNI = pedidos.IdDNI
    -> WHERE pedidos.Estado = 'Pendiente';
